{{> sparql/prefixes.mustache }}

SELECT (REPLACE(STR(?exercise), "^.*\\/([^/]+)$", "$1") AS ?id)
       ?name
WHERE {
  ?exercise a sp:Query ;
    rdfs:label ?name ;
    sp:text ?query ;
    sparqlab:hasDifficulty/skos:notation ?difficulty .
}
ORDER BY ?difficulty STRLEN(?query) {{! Complexity ~ query length }}
